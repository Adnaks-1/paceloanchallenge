{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "static/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/dashboard",
      "dest": "/api/index.py"
    },
    {
      "src": "/",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "maxDuration": 300,
      "memory": 1024,
      "runtime": "python3.13"
    }
  },
  "installCommand": "pip install --upgrade pip setuptools wheel && pip install --no-cache-dir --no-compile -r requirements.txt",
  "buildCommand": "echo 'Build complete'",
  "outputDirectory": ".",
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}

